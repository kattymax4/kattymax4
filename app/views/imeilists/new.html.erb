<div class="container">  
  <div class="row">
    <div class="col-sm-6 col-md-4 col-md-offset-4">
      <h1 class="text-center login-title">Add IMEI number</h1><br><br><br><br>
      <div class="account-wall">

				<%= form_for @imeilist do |f| %>
					<%= f.hidden_field :user_id, value:  current_user.id %>
				  <div>
					  <%= f.label :IMEI %><br>
					  <%= f.text_field :imei, autofocus: true,class: "form-control" %><br><br>
					</div>

				  <div><%= f.submit "Add IMEI" , class: "btn btn-lg btn-block", style: "background-color:#666;color:white" %>
				  </div>
				<% end %>

      </div>
		</div>
	</div>
</div>

